%p= "Amout of women in age 20 to 30 interested: #{interest}"
= link_to new_admin_user_path do
  %button.btn.btn-default
    = "ADD USER"
%table.table.table-hover
  %thead
    %tr
      %th= t('user.columns.email')
      %th= t('user.columns.gender')
      %th= t('user.columns.age')
      %th= t('user.columns.interest')
      %th
  = render users
